{
    "manifest_version": 2,
    "name": "Video Playback Speedup",
    "description": "Control video playback speed using keyboard shortcuts",
    "version": "1.0.0",
    "icons": {
        "128": "images/icon-128.png"
    },
    "browser_action": {
        "default_icon": "images/icon-90.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["scripts/content.js"],
            "all_frames": true
        }

    ],
    "permissions": []
}